package com.practices.matrices;

public class SecondaryDiagonalElement {

	public static void main(String[] args) {
    // Create the 2D matrix
		int[][] matrix = {
				{4, 5, 6},
				{8, 9, 7},
				{10, 8, 7}
		};

		int n = matrix.length;
		System.out.println("Secondary Diagonal Elements:");
		for (int i = 0; i < n; i++) {
			//logic for the printing the secondary diagonal elements.
			System.out.print(matrix[i][n - 1 - i] + " ");
		}
	}
}
